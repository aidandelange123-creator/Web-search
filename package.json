{
  "name": "web-search-ai",
  "version": "2.0.0",
  "description": "A web search application that searches both Bing and DuckDuckGo for real-time results",
  "main": "backend/src/server.js",
  "scripts": {
    "start": "node backend/src/server.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon backend/src/server.js",
    "dev:frontend": "cd frontend && npx serve -s src",
    "backend": "node backend/src/search-engine.js",
    "dev-backend": "nodemon backend/src/search-engine.js",
    "build": "tsc && npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "jest backend/",
    "test:frontend": "jest frontend/",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write ."
  },
  "keywords": [
    "search",
    "bing",
    "duckduckgo",
    "web",
    "search-engine",
    "typescript"
  ],
  "author": "Web Search AI",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.9.3",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "jest": "^29.7.0",
    "eslint": "^8.50.0",
    "prettier": "^3.0.3",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "dotenv": "^16.3.1",
    "helmet": "^7.0.0",
    "rate-limiter-flexible": "^4.0.1",
    "winston": "^3.11.0",
    "moment": "^2.29.4"
  }
}
